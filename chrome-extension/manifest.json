{
  "manifest_version": 3,
  "name": "Mixxx connector",
  "version": "0.1",
  "description": "connect Mixxx and YouTube-VJ.",
  "content_scripts": [
    {
      "js": ["js/script.js"],
      "matches": ["https://kazuprog.github.io/youtube-vj/"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/page.js"],
      "matches": ["https://kazuprog.github.io/*"]
    }
  ]
}
